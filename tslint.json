{
    "extends": "tslint:latest",
    "rules": {
        "curly": true,
        "triple-equals": true,
        "no-console": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never"
                }
            }
        ],
        "max-classes-per-file": [
            true,
            1,
            "exclude-class-expressions"
        ]
    },
    "jsRules": {
        "curly": true
    },
    "rulesDirectory": []
}