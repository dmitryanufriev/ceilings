{
    "extends": "tslint:latest",
    "rules": {
        "curly": true,
        "triple-equals": true,
        "no-console": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never"
                }
            }
        ]
    },
    "jsRules": {
        "curly": true
    },
    "rulesDirectory": []
}